{
    "prettier.enable": false,
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "html.format.wrapAttributes": "force-expand-multiline",
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true
    },
    "files.associations": {
        "*.css": "postcss"
    }
}